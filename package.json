{
  "name": "software-copyright-generator",
  "version": "1.0.0",
  "description": "智能软件著作权登记材料生成器 - 基于AI技术的软著申请文档自动生成工具",
  "main": "server/app.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && nodemon app.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "start": "cd server && node app.js",
    "install:all": "npm install && cd server && npm install && cd ../client && npm install",
    "test": "npm run test:server && npm run test:client",
    "test:server": "cd server && npm test",
    "test:client": "cd client && npm test"
  },
  "keywords": [
    "软件著作权",
    "软著登记",
    "文档生成",
    "AI",
    "自动化",
    "copyright",
    "software-registration",
    "document-generator"
  ],
  "author": "厦门佰能思维人工智能科技有限公司 <contact@bnai.tech>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bnai-tech/software-copyright-generator.git"
  },
  "bugs": {
    "url": "https://github.com/bnai-tech/software-copyright-generator/issues"
  },
  "homepage": "https://github.com/bnai-tech/software-copyright-generator#readme",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}
